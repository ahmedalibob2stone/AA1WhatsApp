platform :ios, '12.0'

ENV['COCOAPODS_DISABLE_STATS'] = 'true'

flutter_application_path = '../'
load File.join(flutter_application_path, '.dart_tool', 'flutter_dependencies.rb') if File.exist?(File.join(flutter_application_path, '.dart_tool', 'flutter_dependencies.rb'))

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  flutter_install_all_ios_pods File.dirname(File.realpath(__FILE__))
end
